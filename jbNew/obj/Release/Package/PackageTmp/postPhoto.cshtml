                             <input type="number" class="form-control footer-input margin-b-20" placeholder="Kattha" name="tbKattha" />
                           <input type="number" class="form-control footer-input margin-b-20" placeholder="Dhur" name="tbDhur" />
                           
                           
                          
                           <button type="submit" class="btn-theme btn-theme-sm btn-base-bg text-uppercase">submit</button>
